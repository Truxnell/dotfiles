# This is Git's per-user configuration file.
[user]
  name = {{ .name }}
  email = {{ .email }}
[format]
  signOff = true
[core]
  editor = nano
  excludesfile = {{ .chezmoi.homeDir }}/.gitignore_global
[merge]
  ff = only
[pull]
  rebase = true
[init]
  defaultBranch = main
[url "ssh://git@github.com/"]
  pushInsteadOf = https://github.com/
